<apex:page controller="importDataFromCSVController">
    <apex:form >
        <apex:pagemessages />
        <apex:pageBlock >
            <apex:pageBlockSection columns="4"> 
                  <apex:inputFile value="{!csvFileBody}"  filename="{!csvFileName}"/>
                  <apex:commandButton value="Import" action="{!importCSVFile}"/>
            </apex:pageBlockSection>
        </apex:pageBlock>
        <apex:pageBlock >
           <apex:pageblocktable value="{!custlist}" var="cw">
              <apex:column value="{!cw.OldId}" />
              <apex:column value="{!cw.NewId}" />
        </apex:pageblocktable>
     </apex:pageBlock>
   </apex:form>
</apex:page>