        <label>Check CSMO Approval</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnectorLabel>Reject</defaultConnectorLabel>
        <rules>
            <name>Approved_and_sent_to_SR</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Step_2_1.Outputs.approvalDecision</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Approve</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.NPV_Discount__c</leftValueReference>
                <operator>GreaterThan</operator>
