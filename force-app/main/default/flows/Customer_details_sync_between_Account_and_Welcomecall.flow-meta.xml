        <expression>ISCHANGED({!$Record.No_of_family_members__c}) || ISCHANGED({!$Record.Zip_Code__c}) || ISCHANGED({!$Record.Occupation__c}) || ISCHANGED({!$Record.Distance_To_Office__c}) || ISCHANGED({!$Record.Company_Name__c}) || ISCHANGED({!$Record.Industry__c}) || ISCHANGED({!$Record.Designation__c}) || ISCHANGED({!$Record.Family_Income__c}) || ISCHANGED({!$Record.Purpose__c}) || ISCHANGED({!$Record.No_of_Vehicles__c}) || ISCHANGED({!$Record.Four_Wheeler__c})</expression>
    </formulas>
    <formulas>
        <name>IsInsert</name>
        <dataType>Boolean</dataType>
        <expression>ISBLANK({!$Record__Prior.Id})</expression>
    </formulas>
    <formulas>
        <name>IsUpdate</name>
        <dataType>Boolean</dataType>
